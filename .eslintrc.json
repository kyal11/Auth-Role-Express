{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prefer-const": "error",
        "no-undef": 0,
        "no-console": "off",
        "semi": ["error", "always"]
    }
}
